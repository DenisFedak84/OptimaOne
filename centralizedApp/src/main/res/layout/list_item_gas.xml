<?xml version="1.0" encoding="utf-8"?>

<TableRow
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:background="#e6e6e6">

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/datastart"
                android:background="@drawable/full_holo_light"
                android:minWidth="120dp"
                android:enabled="false"/>

        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/new_edittext"
                android:id="@+id/datainvisible"
                android:visibility="gone"/>

        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/datafinish"
                android:background="@drawable/full_holo_light"
                android:minWidth="120dp"
                android:enabled="false"/>

        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/value"
                android:background="@drawable/full_holo_light"
                android:minWidth="120dp"
                android:enabled="false"
                android:gravity="center_horizontal"/>

        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:id="@+id/delete"/>
        <EditText android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/gasIntervalId"
                  android:background="@drawable/full_holo_light" android:minWidth="120dp"
                  android:enabled="false" android:gravity="center_horizontal" android:visibility="gone"/>

    </LinearLayout>
</TableRow>